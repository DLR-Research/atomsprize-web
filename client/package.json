{
  "name": "fastprize-client",
  "description": "client for https://fastprize.org",
  "version": "1.0.0",
  "author": "DLR Research",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist/template.html",
    "dist/prerender.js"
  ],
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production && yarn minify-template && yarn build-prerenderer && node scripts/prerender.js",
    "build-prerenderer": "webpack --config webpack.config.prerender.js",
    "minify-template": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/minify_template.ts",
    "start": "http-server dist"
  },
  "devDependencies": {
    "@prefresh/webpack": "^3.3.2",
    "@types/html-minifier": "^4.0.1",
    "@types/node": "^16.10.3",
    "@types/random-seed": "^0.3.3",
    "copy-webpack-plugin": "^9.0.1",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^13.0.2",
    "mini-svg-data-uri": "^1.3.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react-imask": "^6.2.2",
    "tslib": "^2.3.1"
  }
}
