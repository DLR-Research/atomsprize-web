{
  "name": "nobleprize",
  "description": "a collective prize to recognize the scientists who contributed to the COVID-19 mRNA vaccines",
  "version": "1.0.0",
  "author": "DLR Research",
  "license": "MIT",
  "main": "dist/worker.js",
  "scripts": {
    "format": "prettier --write '**/*.{ts,tsx,js,css,json,md,html}'",
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production && yarn minify-template && yarn prerender",
    "build-worker": "yarn minify-template && webpack --mode production --config webpack.config.worker.js",
    "build-worker-dev": "yarn minify-template && webpack --config webpack.config.worker.dev.js",
    "prerender": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/prerenderFromTemplate.ts",
    "minify-template": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/minifyTemplate.ts",
    "start": "http-server dist"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@prefresh/webpack": "^3.3.2",
    "@types/html-minifier": "^4.0.1",
    "@types/node": "^16.10.3",
    "copy-webpack-plugin": "^9.0.1",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^1.17.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "http-server": "^13.0.2",
    "itty-router": "^2.4.4",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react-imask": "^6.2.2",
    "stripe-workers": "^0.11.1-beta",
    "tslib": "^2.3.1"
  }
}
