{
    "name": "fastprize",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "format": "prettier --write './{client,worker}/**/*.{ts,tsx,json,js,yaml,css,html}'",
        "dev:client": "yarn workspace fastprize-client dev",
        "build:client": "yarn workspace fastprize-client build",
        "start:client": "yarn workspace fastprize-client start",
        "deploy:worker": "yarn workspace fastprize-worker deploy"
    },
    "workspaces": {
        "packages": [
            "client",
            "worker"
        ],
        "nohoist": [ "**" ]
    },
    "dependencies": {},
    "devDependencies": {
        "prettier": "^2.4.1"
    }
}
