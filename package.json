{
    "name": "nobleprize",
    "main": "dist/worker.js",
    "targets": {
        "main": false
    },
    "version": "1.0.0",
    "description": "a collective prize to recognize the scientists who contributed to the COVID-19 mRNA vaccines",
    "scripts": {
        "format": "prettier --write '**/*.{js,css,json,md}'",
        "build-worker": "parcel build src/worker.ts",
        "dev": "parcel src/index.html",
        "build": "parcel build src/index.html",
        "start": "http-server dist",
        "typecheck": "tsc --noEmit"
    },
    "author": "DLR Research",
    "license": "MIT",
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    },
    "devDependencies": {
        "@parcel/transformer-image": "2.0.0-rc.0",
        "@types/react": "^17.0.27",
        "@types/react-dom": "^17.0.9",
        "parcel": "^2.0.0-rc.0",
        "prettier": "^1.17.0"
    },
    "dependencies": {
        "http-server": "^13.0.2",
        "preact": "^10.5.14",
        "stripe-workers": "^0.11.1-beta"
    }
}
